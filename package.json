{
  "name": "semester-project-2",
  "version": "0.1.1",
  "description": "Noroff auction house site for semester project 2",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "prepare": "husky install",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olekrr/Semester-Project-2.git"
  },
  "keywords": [
    "js",
    "sass",
    "css",
    "html",
    "bootstrap",
    "prettier",
    "frameworks"
  ],
  "author": "Ole Kristian <oleras94216@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Olekrr/Semester-Project-2/issues"
  },
  "homepage": "https://github.com/Olekrr/Semester-Project-2#readme",
  "dependencies": {
    "bootstrap": "^5.3.2"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.0",
    "sass": "^1.69.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
